    function isBase64(str, imageId) {
        if (str === '' || str.trim() === '') { return false; }
        try {
            debugger;

            var withoutBase64Text = str.replace("data:image/jpeg;base64,", "");

            let patttern = /^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$/;
            let result = withoutBase64Text.match(patttern) ? "1" : "0";

            if (result == "0") {

                if (document.getElementById(imageId).classList.contains('smallDiv')) {
                    document.getElementById(imageId).style.maxHeight = "100%";

                }
                else {
                    document.getElementById(imageId).style.maxHeight = "10000000px";
                }
                //document.getElementById(imageId).style.height = "100%";
                document.getElementById(imageId).src = DefaultErrorImage();
            }
            else {
                document.getElementById(imageId).style.maxHeight = "10000000px";
                document.getElementById(imageId).src = str;
            }
        } catch (err) {
            if (document.getElementById(imageId).classList.contains('smallDiv')) {
                document.getElementById(imageId).style.maxHeight = "100%";

            }
            else {
                document.getElementById(imageId).style.maxHeight = "10000000px";
            }
            document.getElementById(imageId).src = DefaultErrorImage();
            return false;
        }
    }

