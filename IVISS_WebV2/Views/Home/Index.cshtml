    function FitToImage(checkbox, imageId, parentDivId) {


        if (checkbox.checked) {
            debugger;
            document.getElementById(parentDivId).style.overflow = "hidden";
            var clientHeight = document.getElementById(parentDivId).clientHeight;
            var clientWidth = document.getElementById(parentDivId).clientWidth;// document.getElementById(parentDivId).clientWidth;



            var maxWidth = clientWidth; // Max width for the image
            var maxHeight = clientHeight;    // Max height for the image
            var ratio = 0;  // Used for aspect ratio
            var width = $("#" + imageId).width();    // Current image width
            var height = $("#" + imageId).height();  // Current image height

            // Check if the current width is larger than the max
            if ((width - 1) > maxWidth) {
                ratio = maxWidth / width;   // get ratio for scaling image
                $(this).css("width", maxWidth); // Set new width
                $(this).css("height", height * ratio);  // Scale height based on ratio
                height = height * ratio;    // Reset height to match scaled image
                width = width * ratio;    // Reset width to match scaled image
            }


            // Check if current height is larger than max
            if (height > maxHeight) {
                ratio = maxHeight / height; // get ratio for scaling image
                $(this).css("height", maxHeight);   // Set new height
                $(this).css("width", width * ratio);    // Scale width based on ratio
                width = width * ratio;    // Reset width to match scaled image
                height = height * ratio;    // Reset height to match scaled image
            }


            //if (height < 800) {
            //    document.getElementById(imageId).style.maxHeight =  "800px";
            //    document.getElementById(imageId).style.height =  "800px";
            //}
            //else {
            //      document.getElementById(imageId).style.maxHeight = height + "px";
            //}

            document.getElementById(imageId).style.maxHeight = height + "px";
            document.getElementById(imageId).style.maxWidth = width + "px";



        }
        else {
            document.getElementById(parentDivId).style.overflow = "auto";


            document.getElementById(imageId).style.maxHeight = "10000000px";
            document.getElementById(imageId).style.maxWidth = "10000000px";
        }
    }

